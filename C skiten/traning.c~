#include<stdio.h>
#include<string.h>

int isAnagram(char s1, char s2){
  int len1 = strlen(s1);
  int len2 = strlen(s2);
  if(len1 != len2)
    return 0;
  for( int i = 0, i != len1, i++)
    if(strchr(len1[i+1], len2) != 0)
      return 0;
  return 1;
}

int main(int argc, char *argv[]) {
  char string1 = "silent";
  char string2 = "listen";
  if(isAnagram(string1, string2))
    puts("it's an Anagram!");
  else 
    puts("it's not an Anagram");
  return 0;
}
